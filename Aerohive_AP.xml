            <discovery_rules>
                <discovery_rule>
                    <name>Radio</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#AHIFNAME},.1.3.6.1.4.1.26928.1.1.1.2.1.1.1.1]</snmp_oid>
                    <key>ah.Radio.discovery</key>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#AHIFNAME}</macro>
                                <value>wifi.</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>Radio Channel - {#AHIFNAME}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.26928.1.1.1.2.1.5.1.1.{#SNMPINDEX}</snmp_oid>
                            <key>ah.Radio.Channel.[{#AHIFNAME}]</key>
                            <units>ch</units>
                            <applications>
                                <application>
                                    <name>Radio</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{delta(600)}&gt;0</expression>
                                    <name>Changed channel of {#AHIFNAME} from {{HOST.HOST}:{ITEM.KEY}.last(2)} to {ITEM.VALUE}</name>
                                    <priority>INFO</priority>
                                    <description>Changed wireless interface channel.&#13;
{TRIGGER.URL}</description>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Radio TxPower - {#AHIFNAME}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.26928.1.1.1.2.1.5.1.2.{#SNMPINDEX}</snmp_oid>
                            <key>ah.Radio.TxPower.[{#AHIFNAME}]</key>
                            <units>dBm</units>
                            <applications>
                                <application>
                                    <name>Radio</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
