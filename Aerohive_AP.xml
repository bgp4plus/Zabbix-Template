                <item>
                    <name>Cpu Utilization</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.26928.1.2.3.0</snmp_oid>
                    <key>ah.CpuUtilization</key>
                    <units>%</units>
                    <applications>
                        <application>
